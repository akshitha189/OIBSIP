<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Brandify</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
      background: url('https://cdn.shopify.com/s/files/1/0070/7032/articles/trending-products_7ed711c4-206c-4b41-a321-711785f3a917.png?v=1729280622&originalWidth=1848&originalHeight=782&width=1800') no-repeat center center fixed;
      background-size: cover;
      color: #333;
    }
    header {
      background: rgba(255, 255, 255, 0.8); 
      text-align: center;
      padding: 20px;
      position: relative;
    }
    .logo {
      position: absolute;
      top: 10px;
      left: 10px;
    }
    .logo img {
      height: 100px; 
    }
    .top-right-buttons {
      position: absolute;
      top: 10px;
      right: 10px;
    }
    .top-right-buttons .cta-button {
      display: inline-block;
      margin: 5px;
      padding: 10px 20px;
      background: #4CAF50;
      color: white;
      text-decoration: none;
      border-radius: 5px;
      cursor: pointer;
    }
    .top-right-buttons .cta-button:hover {
      background: #45a049;
    }
    header .tagline {
      font-size: 18px;
      color: #555;
      margin-top: 10px;
      margin-bottom: 20px;
    }
    .content {
      text-align: center;
      padding: 50px 20px;
    }
    .products {
      display: flex;
      justify-content: center;
      flex-wrap: wrap;
      gap: 20px;
      padding: 50px;
    }
    .product {
      background-color: rgba(255, 255, 255, 0.7);
      border-radius: 8px;
      padding: 20px;
      width: 250px;
      text-align: center;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
    }
    .product img {
      width: 100%;
      height: auto;
      border-radius: 8px;
    }
    .product h3 {
      margin: 10px 0;
      font-size: 18px;
    }
    .product p {
      font-size: 16px;
      color: #333;
    }
    .product .price {
      font-size: 18px;
      font-weight: bold;
      color: #4CAF50;
    }
    .modal {
      display: none;
      position: fixed;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      background: white;
      padding: 20px;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
      border-radius: 8px;
      z-index: 1000;
    }
    .modal h2 {
      margin-top: 0;
    }
    .modal input {
      display: block;
      width: 100%;
      margin: 10px 0;
      padding: 8px;
      border: 1px solid #ccc;
      border-radius: 4px;
    }
    .modal button {
      background: #4CAF50;
      color: white;
      border: none;
      padding: 10px 20px;
      border-radius: 4px;
      cursor: pointer;
    }
    .modal button:hover {
      background: #45a049;
    }
    #overlay {
      display: none;
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.5);
      z-index: 999;
    }
    #profile-details {
      display: none;
      margin-top: 20px;
      background: rgba(255, 255, 255, 0.8);
      padding: 20px;
      border-radius: 8px;
    }
  </style>
</head>
<body>
  <header>
   
    <div class="logo">
      <img src="https://dynamic.design.com/preview/logodraft/1aa2f349-ec81-4623-836d-0207b76ca3ab/image/large.png" alt="Brandify Logo">
    </div>
    <h1>Welcome to Our Products-BRANDIFY</h1>
    <p class="tagline">Discover the amazing products!</p>
    <div class="top-right-buttons">
      <button id="home-btn" class="cta-button">Home</button>
      <button id="sign-in-btn" class="cta-button">Sign In</button>
      <button id="profile-btn" class="cta-button">Profile</button>
    </div>
  </header>
  <div class="content">
   
    <div class="products">
      <div class="product">
        <img src="data:image/jpeg;base64," alt="Product 1">
        <h3>MEN SHOE</h3>
        <p>This is a great product for everyone!</p>
        <p class="price">2000/-</p>
      </div>
      <div class="product">
        <img src="data:image/jpeg;base64," alt="Product 2">
        <h3>HAIR DRYER</h3>
        <p>High quality and affordable.</p>
        <p class="price">1000/-</p>
      </div>
      <div class="product">
        <img src="https://m.media-amazon.com/images/I/81zuo3QB3mL.jpg" alt="Product 3">
        <h3>LIPSTICK</h3>
        <p>Must-use for your beauty!</p>
        <p class="price">500/-</p>
      </div>
    </div>

    <div id="profile-details">
      <h2>Profile Details</h2>
      <p><strong>Name:</strong> <span id="profile-name"></span></p>
      <p><strong>Email:</strong> <span id="profile-email"></span></p>
      <p><strong>Phone Number:</strong> <span id="profile-phone"></span></p>
    </div>
  </div>

  <div id="overlay"></div>
  <div class="modal" id="sign-in-modal">
    <h2>Sign In</h2>
    <input type="text" id="name" placeholder="Enter your name">
    <input type="email" id="email" placeholder="Enter your email">
    <input type="tel" id="phone" placeholder="Enter your phone number">
    <button id="save-btn">Sign In</button>
  </div>

  <script>
    const homeBtn = document.getElementById('home-btn');
    const signInBtn = document.getElementById('sign-in-btn');
    const profileBtn = document.getElementById('profile-btn');
    const signInModal = document.getElementById('sign-in-modal');
    const overlay = document.getElementById('overlay');
    const saveBtn = document.getElementById('save-btn');
    const profileDetails = document.getElementById('profile-details');
    const profileName = document.getElementById('profile-name');
    const profileEmail = document.getElementById('profile-email');
    const profilePhone = document.getElementById('profile-phone');

    
    signInBtn.addEventListener('click', () => {
      signInModal.style.display = 'block';
      overlay.style.display = 'block';
    });

    
    saveBtn.addEventListener('click', () => {
      const name = document.getElementById('name').value;
      const email = document.getElementById('email').value;
      const phone = document.getElementById('phone').value;

      if (name && email && phone) {
       
        profileName.textContent = name;
        profileEmail.textContent = email;
        profilePhone.textContent = phone;

        signInModal.style.display = 'none';
        overlay.style.display = 'none';

        alert('Details saved successfully!');
      } else {
        alert('Please fill out all fields.');
      }
    });

    
    profileBtn.addEventListener('click', () => {
      profileDetails.style.display = 'block';
    });

    
    overlay.addEventListener('click', () => {
      signInModal.style.display = 'none';
      overlay.style.display = 'none';
    });

   
    homeBtn.addEventListener('click', () => {
      location.reload();
    });
  </script>
</body>
</html>
